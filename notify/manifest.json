{
	"manifest_version":2,
	"name": "Notify",
	"description": "Extension to notify about important staff",
	"version": "0.0.1",
	"browser_action": {
		"default_popup": "popup/main.html"
	},
	"icons": {
		"16": "/img/bell.png",
		"48": "/img/bell.png",
		"128": "/img/bell.png"
	},
	"background": {
		"scripts": ["background.js"]
    },
	"permissions": ["storage","notifications","tabs"]
}